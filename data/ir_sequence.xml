<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Trip Sequence -->
        <record id="seq_mashijai_shipping_trip" model="ir.sequence">
            <field name="name">Shipping Trip Sequence</field>
            <field name="code">mashijai.shipping.trip</field>
            <field name="prefix">TRIP/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Booking Sequence -->
        <record id="seq_mashijai_shipping_booking" model="ir.sequence">
            <field name="name">Shipping Booking Sequence</field>
            <field name="code">mashijai.shipping.booking</field>
            <field name="prefix">BOOK/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>